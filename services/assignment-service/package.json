{
  "name": "creator-assignment-matcher-assignment-service",
  "version": "1.0.0",
  "description": "Assignment Service for Creator Assignment Matcher - Serverless Ready",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "node scripts/migrate.js",
    "serverless:deploy": "serverless deploy",
    "serverless:remove": "serverless remove",
    "serverless:offline": "serverless offline",
    "serverless:logs": "serverless logs -f",
    "docker:build": "docker build -t assignment-service .",
    "docker:run": "docker run -p 3001:3001 assignment-service"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "mongoose": "^7.4.0"
  },
  "devDependencies": {
    "fast-check": "^4.5.3",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^9.5.0",
    "nodemon": "^3.0.1",
    "serverless": "^3.34.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^12.0.4",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "assignment",
    "service",
    "serverless",
    "microservice",
    "mongodb",
    "express"
  ],
  "author": "Creator Assignment Matcher Team",
  "license": "MIT"
}
