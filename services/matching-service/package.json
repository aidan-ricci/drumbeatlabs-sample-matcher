{
  "name": "creator-assignment-matcher-matching-service",
  "version": "1.0.0",
  "description": "Matching Service for Creator Assignment Matcher",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "serverless:deploy": "serverless deploy --verbose",
    "serverless:deploy:dev": "serverless deploy --stage dev --region us-east-1 --verbose",
    "serverless:deploy:prod": "serverless deploy --stage prod --region us-east-1 --verbose",
    "serverless:remove": "serverless remove --verbose",
    "serverless:logs": "serverless logs -f findMatches --tail",
    "serverless:info": "serverless info --verbose",
    "serverless:offline": "serverless offline"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^1.1.0",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "openai": "^4.0.0",
    "@aws-sdk/client-bedrock-runtime": "^3.450.0"
  },
  "devDependencies": {
    "fast-check": "^4.5.3",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "serverless": "^3.38.0",
    "serverless-http": "^3.2.0",
    "serverless-offline": "^13.3.0",
    "serverless-warmup": "^9.0.0",
    "supertest": "^6.3.3"
  }
}